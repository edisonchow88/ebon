<?xml version="1.0"?>
<extension>
    <settings>
        <item id="shipping_storefront_icon">
            <type>resource</type>
            <resource_type>image</resource_type>
            <default_value></default_value>
        </item>    
        <item id="autoselect">
            <type>checkbox</type>
            <default_value>0</default_value>
        </item>
	    <item id="accept_payments">
            <type>checkboxgroup</type>
			<variants>
				<data_source>
                   	<model_rt>setting/extension</model_rt>
                   	<method>getEnabledPayments</method>
				</data_source>
				<fields>
					<field>extension_id</field>
					<field>key</field>
				</fields>
			</variants>
        </item>                
	    <item id="sort_order">
            <type>input</type>
            <default_value></default_value>
        </item>           
    </settings>
</extension>