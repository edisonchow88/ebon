			&lt;td style=&quot;box-sizing: inherit;&quot;&gt;&lt;span style=&quot;color:#000000;&quot;&gt;&lt;img height=&quot;255&quot; src=&quot;http://www.japan-guide.com/g9/5990_03.jpg&quot; style=&quot;box-sizing: inherit; border: 0px; vertical-align: middle;&quot; width=&quot;324&quot; /&gt;&lt;/span&gt;&lt;/td&gt;
